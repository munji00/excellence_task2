{
    "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node10",
    "outDir": "./build",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@modules/*":["*"]
    },
    "esModuleInterop":true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}

